<div class="row">
  {{> admin-menu}}
  <div class="col-md-8">
  <h1>List of all products</h1>
  <ul>
    {{# each products}}
      <li>{{this.name}} -
        <a href="/admin/delete/{{this.id}}"><i class="fa fa-times"></i></a>  -
        <a href="/admin/{{this.id}}/edit"><i class="fa fa-pencil"></i></a>
      </li>
    {{/each}}
  </ul>

  </div>
</div>
